{
  "name": "planning-poker",
  "version": "0.1.0",
  "description": "Planning Poker WebApp",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch .env -x \"node --harmony_proxies\" server.js",
    "build": "npm run build-client",
    "build-client": "cross-env NODE_ENV=production webpack --config ./webpack.prod.js",
    "start": "npm run build-client && cross-env NODE_ENV=production node --harmony_proxies server.js",
    "lint": "xo",
    "test": "cross-env NODE_ENV=test node --harmony_proxies test/runner.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/projeto-spider/planning-poker.git"
  },
  "keywords": [
    "planning",
    "poker",
    "scrum",
    "webapp"
  ],
  "author": "lubien <lubien1996@gmai.com> (http://lubien.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/projeto-spider/planning-poker/issues"
  },
  "homepage": "https://github.com/projeto-spider/planning-poker#readme",
  "private": true,
  "dependencies": {
    "adonis-ace": "^3.0.1",
    "adonis-auth": "^1.0.4",
    "adonis-commands": "^2.1.2",
    "adonis-fold": "^3.0.3",
    "adonis-framework": "^3.0.3",
    "adonis-lucid": "^3.0.8",
    "adonis-middleware": "^1.0.7",
    "adonis-validation-provider": "^3.0.4",
    "aphrodite": "^0.5.0",
    "assets-webpack-plugin": "^3.4.0",
    "axios": "^0.15.1",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.25.0",
    "got": "^6.5.0",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.5",
    "react-dom": "^15.3.2",
    "react-loader": "^2.4.0",
    "react-redux": "^4.4.5",
    "react-redux-loading-bar": "^2.4.0",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-promise-middleware": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-externals-plugin": "^1.0.0",
    "youch": "^1.0.1"
  },
  "autoload": {
    "App": "./app"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "nodemon": "^1.9.1",
    "sqlite": "^2.2.0",
    "xo": "^0.17.0"
  },
  "xo": {
    "global": [
      "use"
    ],
    "rules": {
      "comma-dangle": 0,
      "xo/filename-case": 0,
      "xo/no-process-exit": 0,
      "no-unused-vars": [
        2,
        {
          "args": "all",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "ignores": [
      "client/**/*",
      "public/**/*"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
